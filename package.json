		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"encode",
		"decode",
		"base64"
	],
	"activationEvents": [
		"onCommand:xssencode.StringToBase64",
		"onCommand:xssencode.Base64ToString",
		"onCommand:xssencode.StringToBase32",
